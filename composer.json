{
    "name": "blunck/coupons",
    "description": "Coupon generator for Laravel 5",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Blunck",
            "email": "alex@blunck.co"
        }
    ],
    "require": {
        "php": "^7.1",
        "nesbot/carbon": "^1.22",
        "illuminate/support": "^5.5",
        "illuminate/database": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Blunck\\Coupons\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blunck\\Coupons\\CouponServiceProvider"
            ],
            "aliases": {
                "Coupons": "Blunck\\Coupons\\Facades\\Coupons"
            }
        }
    }
}
